{
  "JwtBearerTokenSettings": {
    "SecretKey": "ThisIsSomeSampleSymmetricEncryptionKey",
    "Audience": "https://localhost:7202/",
    "Issuer": "https://localhost:7202/",
    "ExpiryTimeInSeconds": 10,
    "RefreshTokenExpiryTimeInHours": 48
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "log.txt" }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlConnection": "Server=.;Initial Catalog=BoardGames; Integrated Security=true; MultipleActiveResultSets=True;"
  }
}
